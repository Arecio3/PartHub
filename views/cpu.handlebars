<section class="cpu m-4">
    <h1 class="badge bg-dark ">CPU List</h1>

    <table class="table table-lg table-dark table-striped table-hover">
        <thead>
            <tr>
               
                <th scope="col">Brand</th>
                <th scope="col">Model</th>
                <th scope="col">Benchmark Score</th>
                <th scope="col">Rank</th>
                <th scope="col">samples</th>
            </tr>
        </thead>
        <tbody>
            {{#each cpuList}}
         {{> cpu-details}}
            {{/each}}
        </tbody>
    </table>
</section>


<section class="cpu-pop bg-dark p-4 m-4">
    <h2  class=" cpu-pop-title badge orange text-dark p-3">CPU Popularity</h2>
{{#each cpuPop}}
<h6>{{brand}} {{model}}</h6>
<div class="progress">
    <div class="progress-bar orange" role="progressbar" style="width: {{progress samples}}% " aria-valuenow="100" aria-valuemin="0"
        aria-valuemax="100"></div>
</div>
{{/each}}


</section>